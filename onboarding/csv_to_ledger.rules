# separator
# Define the separator
separator ,

# skip
# Skip headers
skip 1

# date-format
# Format the date correctly (assuming DD/MM/YYYY format)
date-format %d/%m/%Y

# decimal-mark
# Specify decimal mark
decimal-mark .

# fieds list
# Define the format of the CSV file
fields Date, Nature, Amount, DebitCredit, Category, SubCategory

# Field assignment
date %Date
currency EUR 
amount %cur %Amount
description %Nature

# Set default debit account for all transactions
account2 Assets:FR:BforBank:Compte-Courant

# Set default cleared
status *

if
!%Date 04/2024
